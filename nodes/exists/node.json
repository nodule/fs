{
  "name": "exists",
  "ns": "fs",
  "async": true,
  "description": "fs exists",
  "phrases": {
    "active": "Checking whether file {{input.path}} exists"
  },
  "require": {
    "fs": "builtin"
  },
  "ports": {
    "input": {
      "path": {
        "type": "string",
        "async": true
      }
    },
    "output": {
      "yes": {
        "title": "Exists",
        "type": "string"
      },
      "no": {
        "title": "Absent",
        "type": "string"
      }
    }
  }
}
