{
  "name": "mkdir",
  "ns": "fs",
  "description": "fs mkdir",
  "async": true,
  "phrases": {
    "active": "Creating directory {{input.path}}"
  },
  "ports": {
    "input": {
      "path": {
        "title": "Path",
        "type": "string",
        "async": true
      },
      "mode": {
        "title": "Mode",
        "type": "number",
        "default": "0777"
      }
    },
    "output": {
      "path": {
        "title": "Path",
        "type": "string"
      },
      "error": {
        "title": "Error",
        "type": "Error"
      }
    }
  },
  "dependencies": {
    "npm": {
      "fs": "builtin"
    }
  }
}