{
  "name": "mkdir",
  "ns": "fs",
  "description": "fs mkdir",
  "phrases": {
    "active": "Creating directory {{input.path}}"
  },
  "require": {
    "fs": "builtin"
  },
  "ports": {
    "input": {
      "path": {
        "title": "Path",
        "type": "string",
        "required": true
      },
      "mode": {
        "title": "Mode",
        "type": "number",
        "default": "0777"
      }
    },
    "output": {
      "exception": {
        "title": "Exception",
        "type": "object"
      }
    }
  }
}
