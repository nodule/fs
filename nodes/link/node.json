{
  "name": "link",
  "ns": "fs",
  "description": "fs link",
  "phrases": {
    "active": "Linking {{input.srcpath}} to {{input.dstpath}}"
  },
  "require": {
    "fs": "latest"
  },
  "ports": {
    "input": {
      "srcpath": {
        "type": "string"
      },
      "dstpath": {
        "type": "string"
      }
    },
    "output": {
      "exception": {
        "title": "Exception",
        "type": "object"
      },
      "event:complete": {
        "title": "Complete",
        "type": "object"
      }
    }
  }
}
