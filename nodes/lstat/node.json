{
  "name": "lstat",
  "ns": "fs",
  "description": "fs lstat",
  "phrases": {
    "active": "Getting stats for {{input.path}}"
  },
  "require": {
    "fs": "latest"
  },
  "ports": {
    "input": {
      "path": {
        "type": "string"
      }
    },
    "output": {
      "error": {
        "title": "Error",
        "type": "object"
      },
      "stats": {
        "title": "Stats",
        "type": "object"
      }
    }
  }
}
