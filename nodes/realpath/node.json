{
  "name": "realpath",
  "ns": "fs",
  "description": "fs realpath",
  "phrases": {
    "active": "Performing realpath for {{input.path}}"
  },
  "require": {
    "fs": "builtin"
  },
  "ports": {
    "input": {
      "path": {
        "type": "string"
      }
    },
    "output": {
      "error": {
        "title": "Error",
        "type": "object"
      },
      "resolvedPath": {
        "title": "Resolved Path",
        "type": "object"
      }
    }
  }
}
