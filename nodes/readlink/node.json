{
  "name": "readlink",
  "ns": "fs",
  "description": "fs readlink",
  "phrases": {
    "active": "Reading link {{input.path}}"
  },
  "require": {
    "fs": "builtin"
  },
  "ports": {
    "input": {
      "path": {
        "type": "string"
      }
    },
    "output": {
      "error": {
        "type": "object"
      },
      "linkString": {
        "title": "Link String",
        "type": "string"
      }
    }
  }
}
