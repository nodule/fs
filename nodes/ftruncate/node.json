{
  "name": "ftruncate",
  "ns": "fs",
  "description": "fs ftruncate",
  "phrases": {
    "active": "Truncating fd with a length of {{input.len}}"
  },
  "require": {
    "fs": "builtin"
  },
  "ports": {
    "input": {
      "fd": {
        "type": "object"
      },
      "len": {
        "type": "integer"
      }
    },
    "output": {
      "exception": {
        "title": "Exception",
        "type": "object"
      }
    }
  }
}
