{
  "name": "readdir",
  "ns": "fs",
  "description": "fs readdir",
  "phrases": {
    "active": "Listing files from directory {{input.path}}"
  },
  "require": {
    "fs": "builtin"
  },
  "ports": {
    "input": {
      "path": {
        "type": "string"
      }
    },
    "output": {
      "error": {
        "title": "Error",
        "type": "object"
      },
      "files": {
        "title": "Files",
        "type": "array"
      }
    }
  }
}
