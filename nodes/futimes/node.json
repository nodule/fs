{
  "name": "futimes",
  "ns": "fs",
  "description": "fs futimes",
  "phrases": {
    "active": "Set access & modification times for fd to {{input.atime}} and {{input.mtime}}"
  },
  "require": {
    "fs": "builtin"
  },
  "ports": {
    "input": {
      "fd": {
        "type": "object"
      },
      "atime": {
        "type": "string"
      },
      "mtime": {
        "type": "string"
      }
    },
    "output": {
      "exception": {
        "title": "Exception",
        "type": "object"
      },
      "event:complete": {
        "title": "Complete",
        "type": "object"
      }
    }
  }
}
