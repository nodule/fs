{
  "name": "rmdir",
  "ns": "fs",
  "description": "fs rmdir",
  "phrases": {
    "active": "Removing directory {{input.path}}"
  },
  "require": {
    "fs": "latest"
  },
  "ports": {
    "input": {
      "path": {
        "title": "Path",
        "type": "string"
      }
    },
    "output": {
      "exception": {
        "title": "Exception",
        "type": "object"
      },
      "event:complete": {
        "title": "Complete",
        "type": "object"
      }
    }
  }
}
