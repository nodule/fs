{
  "name": "chmod",
  "ns": "fs",
  "description": "fs chmod",
  "phrases": {
    "active": "Changing file mode on {{input.path}}"
  },
  "require": {
    "fs": "latest"
  },
  "ports": {
    "input": {
      "path": {
        "title": "Path",
        "type": "string"
      },
      "mode": {
        "title": "Mode",
        "type": "string"
      }
    },
    "output": {
      "exception": {
        "title": "Exception",
        "type": "object"
      },
      "event:complete": {
        "title": "Complete",
        "type": "object"
      }
    }
  }
}
